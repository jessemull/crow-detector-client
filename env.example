# Sentry Configuration
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
NEXT_PUBLIC_SENTRY_ENVIRONMENT=development
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ENVIRONMENT=development
SENTRY_AUTH_TOKEN_SOURCE_MAPS=your_sentry_auth_token_here
SENTRY_ORG=your_sentry_org_here
SENTRY_PROJECT=your_sentry_project_here
SENTRY_ENVIRONMENT_DEV=test
SENTRY_ENVIRONMENT_TEST=test
SENTRY_ENVIRONMENT_PROD=production

# AWS Configuration (for deployments)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-west-2

# S3 Bucket Names (for CI/CD)
AWS_S3_BUCKET_DEV=crow-detector-client-test
AWS_S3_BUCKET_PROD=crow-detector-client-production
AWS_S3_BUCKET_BACKUP_DEV=crow-detector-client-backup-test
AWS_S3_BUCKET_BACKUP_PROD=crow-detector-client-backup-production

# CloudFront Distribution IDs (for CI/CD)
AWS_CLOUDFRONT_DISTRIBUTION_ID_DEV=your_dev_distribution_id
AWS_CLOUDFRONT_DISTRIBUTION_ID_PROD=your_prod_distribution_id

# CloudFront Configuration (for proxy server)
CLOUDFRONT_DOMAIN=dev.crittercanteen.com
CLOUDFRONT_PRIVATE_KEY=your_cloudfront_private_key_here
CLOUDFRONT_KEY_PAIR_ID=your_cloudfront_key_pair_id_here
CLOUDFRONT_PRIVATE_KEY_PATH=path/to/your/private/key.pem

# Site Configuration
SITE_URL=https://www.crittercanteen.com

# Google Analytics
GA_TRACKING_ID_DEV=your_dev_ga_tracking_id_here
GA_TRACKING_ID_PROD=your_prod_ga_tracking_id_here
NEXT_PUBLIC_GA_TRACKING_ID=your_ga_tracking_id_here

# SSH Configuration (for bastion host access)
SSH_HOST=your_bastion_host_ip
SSH_PRIVATE_KEY_PATH=path/to/your/ssh/private/key
SSH_USER=your_bastion_host_username

# Lighthouse CI
LHCI_GITHUB_APP_TOKEN=your_lighthouse_ci_token_here
